<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WineDishes"
    android:background="@color/wine">

    <Button
        android:id="@+id/whiteMeatButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_marginTop="300dp"
        android:text="White Meat"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"/>

    <Button
        android:id="@+id/redMeatButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="300dp"
        android:text="Red Meat"
        android:layout_marginLeft="10dp"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />

    <Button
        android:id="@+id/fishButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:text="Fish"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="300dp"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />

    <Button
        android:id="@+id/starchButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="200dp"
        android:text="Starches"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />

    <Button
        android:id="@+id/desertButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginTop="200dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:text="Dessert"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />

    <Button
        android:id="@+id/vegetablesButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginTop="400dp"
        android:layout_centerHorizontal="true"
        android:text="Vegetable"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />
    <Button
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:text="Shell Fish"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="200dp"
        android:id="@+id/shellFishButton"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />

    <Button
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:text="Cheese"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_marginTop="400dp"
        android:textSize="14dp"
        android:textStyle="bold"
        android:id="@+id/cheeseButton"
        android:background="@drawable/rounded_button"
        />

    <Button
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:text="Cured Meat"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="400dp"
        android:id="@+id/curedMeatButton"
        android:textSize="14dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_button"
        />




</RelativeLayout>